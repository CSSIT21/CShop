  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@",
              "./src"
            ],
            [
              "~",
              "./src"
            ]
          ],
          "extensions": [
            ".ts",
            ".tsx",
            ".js",
            ".jsx",
            ".json"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.3.0",
    "@vitejs/plugin-react": "^1.0.7",
    "babel-eslint": "^10.1.0",
